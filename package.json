{
  "name": "@codybontecou/pr-error-logger",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript package for logging errors to GitHub PRs with Vercel integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "action.yml",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && cp action.yml dist/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "github",
    "error-logging",
    "pull-request",
    "vercel",
    "typescript",
    "nextjs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codybontecou/pr-error-logger.git"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.0",
    "@octokit/rest": "^20.0.0",
    "@vercel/node": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}